    "2": {
        "40_0": {
            "lines": [
                "You've come to this place."
            ]
        },
        "120_0": {
            "lines": [
                "Those three are just useless..."
            ]
        },
        "210_0": {
            "lines": [
                "There's just one option left..."
            ]
        },
        "300_0": {
            "lines": [
                "I shall defeat you!"
            ]
        }
